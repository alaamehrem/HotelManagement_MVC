@model IEnumerable<HotelRoomType>

       @foreach(var item in Model){
    <div class="col-lg-4 col-md-6">
        <div class="room-item">
    
            <div class="hp-room-item set-bg" data-setbg="/Images/RoomType/@item.Images">
                <div class="hr-text">
                    <h3>@item.Name</h3>
                    <h2>@item.Price LE <span> /Pernight</span></h2>
                    <table>
                        <tbody>
                            <tr>
                                <td class="r-o">Size: </td>
                                <td>@item.Area m2</td>
                            </tr>
                            <tr>
                                <td class="r-o">Capacity: </td>
                                <td>@item.MaxGuestCount</td>
                            </tr>
                            <tr>
                                <td class="r-o">Bed numbers: </td>
                                <td>@item.BedCount</td>
                            </tr>
                            <tr>
                                <td class="r-o">View:  </td>
                                <td>@item.View </td>
                            </tr>
                        </tbody>
                    </table>
                    <a href="/HotelRoomType/Details?id=@item.Id" class="primary-btn">More Details</a>
                </div>
            </div>
         
        </div>
    </div>
    }

