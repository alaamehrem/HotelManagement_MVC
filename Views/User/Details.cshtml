@model UserViewModel

@{
    ViewData["Title"] = "Details";
    Layout = "_Dashboard";
};


<div class="container">
    <div class="row">
        <div class="col-lg-12">
            <div class="breadcrumb-text">
                <h2>Room Details</h2>
                <div class="bt-option">
                    <a asp-area="" asp-controller="Home" asp-action="Index">Home</a>
                    <a asp-area="" asp-controller="User" asp-action="Index">User Data</a>
                    <span>User Details</span>
                </div>
            </div>
        </div>
    </div>
</div>
<br />
<!-- Room Details Section Begin -->
<section class="room-details-section spad">
    <div class="container">
        <div class="row">
            <div class="col-lg-8">
                <div class="room-details-item">
                    <div class="rd-text">
                        <div class="rd-title">
                            <h3>@Model.FName</h3>
                            <br />
                            <hr />
                        <table>
                            <tbody>
                                    <tr>
                                        <td class="r-o">Id : </td>
                                        <td> @Model.Id</td>
                                    </tr>
                                <tr>
                                    <td class="r-o">First Name : </td>
                                    <td> @Model.FName</td>
                                </tr>
                                <tr>
                                    <td class="r-o">Last Name : </td>
                                    <td> @Model.LName</td>
                                </tr>
                                    <tr>
                                        <td class="r-o">Phone Number : </td>
                                        <td> @Model.PhoneNumber</td>
                                    </tr>
                                    <tr>
                                        <td class="r-o">Email : </td>
                                        <td> @Model.Email</td>
                                    </tr>
                            </tbody>
                        </table>
                    </div>
                </div>
            </div>
        </div>
    </div>
</section>